<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.ExamAccessController">
    <children>
        <TableView fx:id="examsTable">
            <columns>
                <TableColumn fx:id="titleColumn" text="Title" />
                <TableColumn fx:id="subjectColumn" text="Subject" />
                <TableColumn fx:id="durationColumn" text="Duration (mins)" />
            </columns>
        </TableView>
        <Button text="Start Exam" onAction="#handleStartExam" />
    </children>
</VBox>
