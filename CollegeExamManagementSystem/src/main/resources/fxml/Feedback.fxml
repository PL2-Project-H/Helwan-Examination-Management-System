<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.FeedbackController">
    <children>
        <Label text="Enter your feedback:" />
        <TextArea fx:id="feedbackTextArea" prefRowCount="10" />
        <Button text="Submit Feedback" onAction="#handleSubmitFeedback" />
    </children>
</VBox>
